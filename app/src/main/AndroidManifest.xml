<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.machinescanner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".about_us_page"></activity>
        <activity android:name=".about_app_page" />
        <activity android:name=".contact_page" />
        <activity android:name=".instruction_page" />
        <activity android:name=".help_page" />
        <activity android:name=".main_page" />
        <activity android:name=".setting_page" />
        <activity android:name=".machine_detail_page" />
        <activity android:name=".signup_page" />
        <activity android:name=".login_page">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>